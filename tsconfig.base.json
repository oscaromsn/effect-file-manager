{
  "compilerOptions": {
    "strict": true,
    "exactOptionalPropertyTypes": true,
    "moduleDetection": "force",
    "composite": true,
    "downlevelIteration": true,
    "resolveJsonModule": true,
    "esModuleInterop": false,
    "declaration": true,
    "skipLibCheck": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "bundler",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "types": [],
    "isolatedModules": true,
    "sourceMap": true,
    "declarationMap": true,
    "noImplicitReturns": false,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "noFallthroughCasesInSwitch": true,
    "noEmitOnError": false,
    "noErrorTruncation": true,
    "allowJs": false,
    "checkJs": false,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUncheckedIndexedAccess": true,
    "strictNullChecks": true,
    "target": "ES2022",
    "module": "ESNext",
    "incremental": true,
    "removeComments": false,
    "plugins": [
      {
        "name": "@effect/language-service",
        "quickinfoEffectParameters": "always",
        "namespaceImportPackages": ["effect", "@effect/*"],
        "topLevelNamedReexports": "follow",
        "diagnosticSeverity": {
          "importFromBarrel": "error"
        },
        "importAliases": {
          "Array": "Arr",
          "Function": "Func",
          "Boolean": "Bool",
          "Number": "Num",
          "String": "Str"
        }
      }
    ],
    "paths": {
      "@example/domain": ["./packages/domain/src/index.js"],
      "@example/domain/*": ["./packages/domain/src/*.js"],
      "@example/server": ["./packages/server/src/index.js"],
      "@example/server/*": ["./packages/server/src/*.js"]
    }
  }
}
